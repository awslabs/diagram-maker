{
  "name": "diagram-maker",
  "description": "A library to display an interactive editor for any graph-like data.",
  "version": "1.1.0",
  "author": {
    "name": "Amazon Web Services",
    "url": "https://aws.amazon.com/"
  },
  "homepage": "https://awslabs.github.io/diagram-maker",
  "contributors": [
    "Sameer Goyal <sameeg@amazon.com>",
    "Matthew Oakes <mattoa@amazon.com>"
  ],
  "engines": {
    "node": ">=8"
  },
  "main": "dist/diagramMaker.js",
  "types": "dist/diagramMaker.d.ts",
  "typings": "dist/diagramMaker.d.ts",
  "license": "Apache-2.0",
  "keywords": [
    "aws",
    "visualizaton",
    "diagram",
    "interactive",
    "canvas",
    "editor",
    "drag",
    "drop",
    "workflow",
    "state-machine",
    "flowchart",
    "diagram-maker"
  ],
  "devDependencies": {
    "@cypress/code-coverage": "^3.8.1",
    "@types/classnames": "2.2.x",
    "@types/dagre": "0.7.x",
    "@types/jest": "26.0.x",
    "@types/lodash-es": "4.17.x",
    "@types/uuid": "8.x",
    "@types/webpack": "4.41.x",
    "@types/webpack-env": "1.15.x",
    "autoprefixer": "9.8.x",
    "classnames": "2.2.x",
    "css-loader": "4.2.x",
    "cypress": "5.0.x",
    "dagre": "^0.8.5",
    "dts-bundle-generator": "5.4.x",
    "es6-shim": "0.35.x",
    "fork-ts-checker-webpack-plugin": "5.1.x",
    "html-webpack-plugin": "4.3.x",
    "immer": "7.0.x",
    "istanbul-instrumenter-loader": "3.0.x",
    "istanbul-lib-coverage": "3.0.x",
    "jest": "26.4.x",
    "jest-snapshot": "26.4.x",
    "lodash-es": "4.17.x",
    "mini-css-extract-plugin": "0.11.x",
    "node-sass": "^4.14.1",
    "nyc": "15.x",
    "optimize-css-assets-webpack-plugin": "5.0.x",
    "postcss": "7.0.x",
    "postcss-loader": "3.0.x",
    "postcss-scss": "2.1.x",
    "preact": "8.5.x",
    "preact-redux": "2.0.x",
    "preact-render-spy": "1.x",
    "redux": "4.0.x",
    "redux-undo-redo": "2.1.x",
    "sass-loader": "10.0.x",
    "ts-jest": "26.3.x",
    "ts-loader": "8.0.x",
    "tslint": "5.11.x",
    "tslint-config-airbnb": "5.9.x",
    "tslint-eslint-rules": "5.3.x",
    "tslint-react": "3.6.x",
    "typedoc": "^0.19.0",
    "typescript": "4.x",
    "uuid": "8.x",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "5.1.x"
  },
  "scripts": {
    "build": "webpack --config webpack.prod.js",
    "clean": "rm -rf ./build && rm -rf ./coverage && rm -rf ./dist",
    "documentation": "typedoc src/ --gitRemote upstream --gitRevision master",
    "start": "webpack-dev-server --config webpack.dev.js",
    "server": "npm run start",
    "server:remote": "REMOTE=true npm run start",
    "lint": "tslint --config tslint.json --project tsconfig.json -t stylish",
    "lint:fix": "tslint --config tslint.json --project tsconfig.json -t stylish --fix",
    "test": "tsc --noEmit -p . && jest --config jest.json",
    "test:dev": "tsc --noEmit -p . && jest --config jest.json --watch",
    "test:update": "tsc --noEmit -p . && jest --config jest.json --updateSnapshot",
    "copy:demo": "cp -r dist/examples docs",
    "release": "npm run lint && npm run test && npm run documentation && npm run build && npm run release:integ && npm run copy:demo",
    "build:integ": "webpack --config webpack.static.js",
    "lint:integ": "tslint --config tslint.json --project ./cypress/tsconfig.json -t stylish",
    "lint:integ:fix": "tslint --config tslint.json --project ./cypress/tsconfig.json -t stylish --fix",
    "test:integ": "npm run build:integ && tsc --noEmit -p ./cypress && cypress run --env coverage=true",
    "test:integ:dev": "tsc --noEmit -p ./cypress && cypress open --config baseUrl=http://localhost:31213 --env coverage=true",
    "release:integ": "npm run build:integ && npm run lint:integ && tsc --noEmit -p ./cypress && npm run test:integ",
    "prepublishOnly": "npm run clean && npm run release"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/awslabs/diagram-maker"
  }
}
