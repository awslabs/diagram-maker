{
  "extends": [
    "eslint:recommended",
    "plugin:import/recommended",
    "airbnb",
    "airbnb-typescript",
    "plugin:cypress/recommended"
  ],
  "plugins": [
    "@typescript-eslint",
    "cypress"
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "project": "cypress/tsconfig.json"
  },
  "rules": {
    "max-len": ["error", { "code": 120 }]
  }
}
